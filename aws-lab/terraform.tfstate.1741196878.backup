    {
      "module": "module.bastion",
      "mode": "managed",
      "type": "local_file",
      "name": "db_config_script",
      "provider": "provider[\"registry.terraform.io/hashicorp/local\"]",
      "instances": [
        {
          "index_key": 0,
          "schema_version": 0,
          "attributes": {
            "content": "    #!/bin/bash\n    # Wait for instance to be fully ready\n    echo \"Waiting for instance i-09e8012966221bbb6 to be ready for SSM...\"\n    aws ec2 wait instance-status-ok --instance-ids i-09e8012966221bbb6 --region us-east-1\n    \n    # Create the temporary script on the local machine\n    cat \u003e /tmp/configure-db-access.sh \u003c\u003c 'SCRIPT'\n#!/bin/bash\nDB_ENDPOINT=\"aurora-cluster-1-multi-az.cluster-cv244s608wpb.us-east-1.rds.amazonaws.com\"\nREGION=\"us-east-1\"\n\n# Store the endpoint in SSM Parameter Store\necho \"Storing database endpoint in SSM: $DB_ENDPOINT\"\naws ssm put-parameter \\\n  --name \"/aurora/endpoint\" \\\n  --value \"$DB_ENDPOINT\" \\\n  --type \"String\" \\\n  --overwrite\n\n# Create a connection script for convenience\ncat \u003e /home/ec2-user/connect-db.sh \u003c\u003c EOT\n#!/bin/bash\n# Script to connect to PostgreSQL using IAM authentication\n\nDB_ENDPOINT=\"$DB_ENDPOINT\"\nREGION=\"$REGION\"\nUSER=\"iam_db_user\"\nDATABASE=\"postgres\"\n\necho \"Generating authentication token...\"\nTOKEN=\\$(aws rds generate-db-auth-token \\\\\n  --hostname \\$DB_ENDPOINT \\\\\n  --port 5432 \\\\\n  --region \\$REGION \\\\\n  --username \\$USER)\n\necho \"Connecting to PostgreSQL database...\"\nPGPASSWORD=\\$TOKEN psql \"host=\\$DB_ENDPOINT user=\\$USER dbname=\\$DATABASE sslmode=require\"\nEOT\n\nchmod +x /home/ec2-user/connect-db.sh\necho \"Database access configuration complete.\"\nSCRIPT\n    \n    # Make it executable\n    chmod +x /tmp/configure-db-access.sh\n    \n    # Try up to 3 times to run the command (with 30 second delays)\n    for i in {1..3}; do\n      echo \"Attempt $i: Running SSM command on instance i-09e8012966221bbb6...\"\n      if aws ssm send-command \\\n        --instance-ids \"i-09e8012966221bbb6\" \\\n        --document-name \"AWS-RunShellScript\" \\\n        --parameters \"commands=[\\\"bash -ex /tmp/configure-db-access.sh\\\"]\" \\\n        --region \"us-east-1\"; then\n        echo \"Success! SSM command executed.\"\n        exit 0\n      else\n        echo \"Attempt $i failed. Waiting 30 seconds before retry...\"\n        sleep 30\n      fi\n    done\n    \n    echo \"Warning: Failed to configure database access after 3 attempts.\"\n    echo \"You may need to manually configure it later.\"\n    exit 1\n",
